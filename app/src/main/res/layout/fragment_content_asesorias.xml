<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Secundaria.fragments.contentAsesoriasFragment">

    <android.support.v4.view.ViewPager
        android:id="@+id/pagerasesorias"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="60dp"

        />

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/main_bottomasesorias"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/barbottom"
        app:itemIconTint="@drawable/colorbottombalotario"
        app:itemTextColor="@drawable/colorbottombalotario"
        app:menu="@menu/menu_seminarios"
        android:layout_alignParentBottom="true"
        >
    </android.support.design.widget.BottomNavigationView>


</RelativeLayout>