<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Secundaria.fragmentResueltos">


    <ImageView
        android:id="@+id/img_backResueltos"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ic_arrow" />

    <TextView
        android:id="@+id/tx_titlePropuesto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PROBLEMAS RESUELTOS"
        android:layout_marginTop="20dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="15dp"
        android:textSize="25sp" />


    <GridView

        android:id="@+id/ma_gridResueltos"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@id/tx_titlePropuesto"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="3dp"
        android:horizontalSpacing="1dp"
        android:numColumns="2"
        android:verticalSpacing="1dp"

        >

    </GridView>

</RelativeLayout>