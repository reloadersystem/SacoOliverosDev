<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Primaria.fragments.asesoriaFragment">


    <TextView
        android:id="@+id/tx_tomoMulti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        android:text="ASESORIAS  VIRTUALES"
        android:fontFamily="@font/rajdhanisemibold"
        android:textSize="30sp"

         />

    <GridView
        android:layout_gravity="center_horizontal|center_vertical"
        android:id="@+id/ma_asesoria"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:horizontalSpacing="1dp"
        android:numColumns="2"
        android:verticalSpacing="1dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/tx_tomoMulti"

        >

    </GridView>



</RelativeLayout>